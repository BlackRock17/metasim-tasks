# Azure OpenAI Configuration
# Get these values from Azure Portal -> Your OpenAI Service -> Keys and Endpoint

# Your API Key (use KEY 1 or KEY 2 from Azure Portal)
AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here

# Your Azure OpenAI Endpoint
AZURE_OPENAI_ENDPOINT=https://your-service-name.openai.azure.com/

# Your Deployment Name (the name you gave to your GPT-4o deployment)
AZURE_OPENAI_DEPLOYMENT=your_deployment_name

# API Version (LangChain recommended version)
AZURE_OPENAI_API_VERSION=2024-10-21

# FastAPI Configuration
FASTAPI_HOST=0.0.0.0
FASTAPI_PORT=8000
FASTAPI_DEBUG=True

# Logging Level
LOG_LEVEL=INFO